#!/Users/Amith/video/lss-app/envi/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'lss','console_scripts','lss'
__requires__ = 'lss'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lss', 'console_scripts', 'lss')()
    )
